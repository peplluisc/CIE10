      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>/*-----------------------------------------------------------------------
   Entradas (númericas 0/1)  : HOMBRE, MUJER, PERINATAL, PEDIATRICO, ADULTO
   Entrada (string, diagnóstico): JUICIO_CLINICO
   Salida  : JUICIO_CLINICO_ENRIQUECIDO
 -----------------------------------------------------------------------*/

function isTrue(v) {       // convierte 0/1/null a booleano
  return v != null &amp;&amp; Number(v) === 1;
}

/* --- Género --------------------------------------------------------- */
var generoTxt = '';
if      (isTrue(HOMBRE)) generoTxt = 'Paciente masculino ';
else if (isTrue(MUJER))  generoTxt = 'Paciente femenino ';
else                     generoTxt = 'Paciente sexo indefinido ';

/* --- Rango de edad -------------------------------------------------- */
var edadTxt = '';
if      (isTrue(ADULTO))     edadTxt = 'adulto. ';
else if (isTrue(PERINATAL))  edadTxt = 'perinatal. ';
else if (isTrue(PEDIATRICO)) edadTxt = 'pediatrico. ';
else                         edadTxt = 'edad indefinida. ';

/* --- Diagnóstico ---------------------------------------------------- */
var diag = (JUICIO_CLINICO == null ? '' : JUICIO_CLINICO.toLowerCase());

/* --- Campo enriquecido ---------------------------------------------- */
var JUICIO_CLINICO_ENRIQUECIDO = generoTxt + edadTxt + diag ;
</jsScript_script>
      </jsScript>
