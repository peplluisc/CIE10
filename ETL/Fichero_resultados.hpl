    <sql>SELECT 
    cod.IN_ID_EPISODIO,
    cod.STATUS_EDAD,
    cod.IN_SEXO,
    cod.IN_JUICIO_CLINICO,
    cod.CODIFICADO_CIE            AS Descripcion_CIE,
    cod.CODIFICADO_CIE_CODIGO     AS Codigo_CIE,
    cod.IN_FICHERO_ORIGEN         AS Fichero_Origen,
    cod.CODIFICADO_TIPO           AS Tipo_Codificacion,
    cod.CODIFICADO_CONFIANZA      AS Confianza,
    CASE 
        WHEN err.IN_ID_EPISODIO IS NOT NULL THEN 'Sí'
        ELSE 'No'
    END AS ERROR,
    CASE 
        WHEN cod.CODIFICADO_CIE_CODIGO IS NOT NULL THEN 'Sí'
        ELSE 'No'
    END AS CODIFICADO
FROM 
    ZM.ZT_DOCLIB_CIE10_CODIFICAR cod
LEFT JOIN 
    ZM.ZT_DOCLIB_CIE10_ERRORES err
    ON err.IN_ID_EPISODIO = cod.IN_ID_EPISODIO</sql>
