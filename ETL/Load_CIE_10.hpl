    <execute_each_row>N</execute_each_row>
    <sql>SELECT 
    CASE WHEN TO_CHAR(c.sexo) = '1' THEN 1 ELSE 0 END AS HOMBRE,
    CASE WHEN TO_CHAR(c.sexo) = '2' THEN 1 ELSE 0 END AS MUJER,
    CASE WHEN TO_CHAR(c.STATUS_EDAD) = 'PERINATAL' THEN 1 ELSE 0 END AS PERINATAL,
    CASE WHEN TO_CHAR(c.STATUS_EDAD) = 'PEDIATRICO' THEN 1 ELSE 0 END AS PEDIATRICO,
    CASE WHEN TO_CHAR(c.STATUS_EDAD) = 'ADULTO' THEN 1 ELSE 0 END AS ADULTO,
    R.literal_limpio AS JUICIO_CLINICO,
    T1.DESCRIPCION_tratado AS CIE_ASIGNADO,
    T1.CODIGO AS CODIGO_CIE_ASIGNADO,
    NVL(T1.HOMBRE, 0) AS HOMBRE_REQUERIDO,
    NVL(T1.MUJER, 0) AS MUJER_REQUERIDO,
    NVL(T1.PERINATAL, 0) AS PERINATAL_REQUERIDO,
    NVL(T1.PEDIATRICO, 0) AS PEDIATRICO_REQUERIDO,
    NVL(T1.ADULTO, 0) AS ADULTO_REQUERIDO,
    c.HISTORIA AS HISTORIA,
    c.FILA AS FILA,
    R.FICHERO AS FICHERO
FROM 
    ZM.ZT_DOCLIB_CIE10_REGISTROS_TRATADOS R
LEFT JOIN 
    ZM.ZT_DOCLIB_CIE10_CMDB c ON R.identificador = c.numepi
LEFT JOIN 
    ZM.ZT_DOCLIB_CIE10_TABLA_REFERENCIA T1 ON T1.CODIGO = R.CIE_1
WHERE 
    R.cie_1 IS NOT NULL
</sql>
