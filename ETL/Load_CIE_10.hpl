      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

// Inicializamos los campos
var error = 0;
var error_descripcion = "";

// Solo comprobamos si hay un CIE asignado
if (CIE_ASIGNADO != null &amp;&amp; CIE_ASIGNADO.trim() != "") {

  var errores = [];

  // Validamos cada requisito del CIE con los datos del paciente
  if (HOMBRE_REQUERIDO == 1 &amp;&amp; HOMBRE != 1) {
    errores.push("CIE requiere hombre pero el paciente no es hombre");
  }

  if (MUJER_REQUERIDO == 1 &amp;&amp; MUJER != 1) {
    errores.push("CIE requiere mujer pero el paciente no es mujer");
  }

  if (PERINATAL_REQUERIDO == 1 &amp;&amp; PERINATAL != 1) {
    errores.push("CIE requiere perinatal pero el paciente no es perinatal");
  }

  if (PEDIATRICO_REQUERIDO == 1 &amp;&amp; PEDIATRICO != 1) {
    errores.push("CIE requiere pediátrico pero el paciente no es pediátrico");
  }

  if (ADULTO_REQUERIDO == 1 &amp;&amp; ADULTO != 1) {
    errores.push("CIE requiere adulto pero el paciente no es adulto");
  }

  // Si hay errores, marcamos el campo error y creamos la descripción
  if (errores.length > 0) {
    error = 1;
    error_descripcion = errores.join("; ");
  }
}

// Asignamos los campos de salida
error = error;
error_descripcion = error_descripcion;
</jsScript_script>
